{
  "name": "react-display-switch",
  "version": "0.0.2",
  "description": "React Components for improving readability by eliminating { }, && and ternary operators out of JSX",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:babel": "babel src/ --out-dir lib/ --minified --source-maps inline",
    "build:sample": "browserify --verbose -t babelify sample/index.js -o sample/index.bundle.js",
    "test": "run-s test:*",
    "test:standard": "standard"
  },
  "keywords": [
    "react",
    "jsx"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "/lib"
    ]
  }
}
